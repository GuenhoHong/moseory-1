<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="product">
	<insert id="regist" parameterType="map">
		insert into tbl_product values(
			#{code},
			#{name},
			#{high_code},
			#{low_code},
			#{price},
			0,
			0,
			0,
			#{file_path},
			#{file_name},
			#{product_comment}
		)
	</insert>
	
</mapper>
